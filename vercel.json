{
  "version": 2,
  "builds": [
    { "src": "api/search.js", "use": "@vercel/node" },
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "autoplaysong.mp3", "use": "@vercel/static" },
    { "src": "automusic.mp3", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/search", "dest": "/api/search.js" },
    { "src": "/autoplaysong.mp3", "dest": "/autoplaysong.mp3" },
    { "src": "/automusic.mp3", "dest": "/automusic.mp3" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
